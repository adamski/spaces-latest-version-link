# DigitalOcean Spaces credentials
SPACES_KEY=your_spaces_access_key
SPACES_SECRET=your_spaces_secret_key
SPACES_REGION=nyc3

# Default bucket and prefix (optional - can be passed as query params)
SPACES_BUCKET=your-bucket-name
SPACES_PREFIX=installers/

# Optional file pattern to match (regex)
FILE_PATTERN=.*\.(exe|dmg|deb|rpm)$
